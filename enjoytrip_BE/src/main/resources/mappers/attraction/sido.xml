<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssafy.trip.domain.attraction.repository.SidoDao">

	<select id="selectByCode">
		select * from sido where sido_code = #{sidoCode}
	</select>
	
	<select id="select">
		select * from sido where id = #{id}
	</select>

	<select id="selectAll">
		select * from sido;
	</select>

	<resultMap id="sidoMap" type="Sido">
		<id column="id" property="id" />
		<result column="sido_code" property="sidoCode" />
		<result column="sido_name" property="sidoName" />
	</resultMap>
	
</mapper>